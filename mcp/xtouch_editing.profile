<?xml version="1.0" encoding="UTF-8"?>
<MackieDeviceProfile>
  <Name value="Behringer XTouch (editing)"/>
  <Buttons>
    <Button name="Marker" plain="Editor/jump-forward-to-mark"/>
    <Button name="Nudge" plain="Region/nudge-forward" control="Region/naturalize-region" shift="Region/nudge-backward" option="Transport/ToggleExternalSync"/>
    <Button name="Drop" plain="Editor/start-range-from-playhead"/>
    <Button name="Replace" plain="Editor/end-range-from-playhead"/>
    <Button name="Clear Solo" plain="Main/cancel-solo"/>
